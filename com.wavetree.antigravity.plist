-->
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.wavetree.antigravity</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/zsh</string>
		<string>-lc</string>
		<string>exec /usr/bin/python3 /Volumes/AI_DATA_CENTRE/AI_WORKSPACE/woonmok.github.io/antigravity.py</string>
	</array>
	<key>WorkingDirectory</key>
	<string>/Users/seunghoonoh</string>
	<key>StandardOutPath</key>
	<string>/tmp/com.wavetree.antigravity.out.log</string>
	<key>StandardErrorPath</key>
	<string>/tmp/com.wavetree.antigravity.err.log</string>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<key>RestartDelay</key>
	<integer>10</integer>
</dict>
</plist>
